<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary"
    tools:context=".maintecnico.CrearTiket">

    <ImageView
        android:id="@+id/img_cre_atras"
        android:layout_width="@dimen/_25mdp"
        android:layout_height="@dimen/_20mdp"
        android:layout_marginTop="@dimen/_14mdp"
        android:layout_marginStart="@dimen/_10mdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_atras" />


    <TextView
        android:id="@+id/txt_tit_cre_tik"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Escoge Cliente"
        android:textColor="@color/colorBlanco"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/img_cre_atras"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/img_cre_atras"
        app:layout_constraintTop_toTopOf="@+id/img_cre_atras" />

    <co.tecniprint.tecniprinttecnicos.otros.NonSwipeableViewPager
        android:id="@+id/vpg_cre_tik"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_40mdp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.016">


    </co.tecniprint.tecniprinttecnicos.otros.NonSwipeableViewPager>


</androidx.constraintlayout.widget.ConstraintLayout>